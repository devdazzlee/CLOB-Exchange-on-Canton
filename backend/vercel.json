{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/match/trigger",
      "schedule": "* * * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NODE_TLS_REJECT_UNAUTHORIZED": "0"
  }
}
