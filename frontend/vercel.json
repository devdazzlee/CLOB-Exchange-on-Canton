{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/proxy/keycloak-token",
      "destination": "/api/proxy"
    },
    {
      "source": "/api/proxy/(.*)",
      "destination": "/api/proxy"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/backend"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/proxy.js": {
      "runtime": "@vercel/node@3.2.0"
    },
    "api/backend.js": {
      "runtime": "@vercel/node@3.2.0",
      "maxDuration": 30
    }
  }
}
